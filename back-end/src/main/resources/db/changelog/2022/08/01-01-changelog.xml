<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
                      http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.5.xsd"
        objectQuotingStrategy="QUOTE_ONLY_RESERVED_WORDS">
    <changeSet id="1656851034029-1" author="yomna">
        <createTable tableName="follow_up">
            <column autoIncrement="true" name="id" type="INT">
                <constraints nullable="false" primaryKey="true" primaryKeyName="pk_followup"/>
            </column>
            <column name="version" type="INT"/>
            <column name="created_by" type="VARCHAR(255)"/>
            <column name="modified_by" type="VARCHAR(255)"/>
            <column name="created_date" type="DATETIME"/>
            <column name="modified_date" type="DATETIME"/>
            <column name="interview_id" type="INT"/>
        </createTable>
    </changeSet>
    <changeSet id="1656851034027-19" author="yomna">
        <addForeignKeyConstraint baseColumnNames="interview_id" baseTableName="follow_up"
                                 constraintName="FK_FOLLOWUP_ON_INTERVIEW" referencedColumnNames="id"
                                 referencedTableName="interview"/>
    </changeSet>
</databaseChangeLog>